{
  "version": "0.3.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "yarn clean-build && npx rollup --config --configDebug -w",
    "build": "yarn clean-build && npx rollup --config --configDebug",
    "clean-build": "rm -rf dist/*.js && rm -rf dist/*.map && rm -rf dist/**/*.js && rm -rf dist/**/*.map",
    "start-old": "tsdx watch",
    "build-old": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "install-peers && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-bricks": "^3.2.4",
    "prismjs": "^1.25.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-bricks-ui",
  "author": "Matteo Frana <info@reactbricks.com>",
  "module": "dist/react-bricks-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/react-bricks-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-bricks-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/classnames": "^2.3.1",
    "@types/jsonp": "^0.2.1",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "babel-plugin-prismjs": "^2.1.0",
    "install-peers-cli": "^2.2.0",
    "react": ">=17",
    "react-dom": "^17.0.2",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "email-validator": "^2.0.4",
    "jsonp": "^0.2.1",
    "pigeon-maps": "^0.19.7",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.25.0",
    "react-icons": "^4.2.0",
    "react-leaflet": "^3.2.1",
    "react-simple-code-editor": "^0.11.0"
  }
}
