{
  "version": "0.1.14",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "install-peers && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-bricks": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-bricks-ui",
  "author": "Matteo Frana <info@reactbricks.com>",
  "module": "dist/react-bricks-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/react-bricks-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-bricks-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.8.0",
    "@types/classnames": "^2.2.11",
    "@types/jsonp": "^0.2.0",
    "@types/leaflet": "^1.5.19",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-plugin-prismjs": "^2.0.1",
    "husky": "^4.3.0",
    "install-peers-cli": "^2.2.0",
    "react": ">=16",
    "react-dom": "^17.0.1",
    "size-limit": "^4.8.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "email-validator": "^2.0.4",
    "jsonp": "^0.2.1",
    "leaflet": "^1.7.1",
    "mapbox-gl": "^2.1.0",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.23.0",
    "react-bricks": "^2.0.0",
    "react-icons": "^4.1.0",
    "react-leaflet": "^3.0.2",
    "react-simple-code-editor": "^0.11.0"
  }
}
